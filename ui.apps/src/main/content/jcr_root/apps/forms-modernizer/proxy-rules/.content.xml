              sling:resourceType="appId/components/adaptiveForm/termsandconditions"
              cq:copyChildren="{Boolean}true"
              cq:rewriteFinal="{Boolean}true"
              fieldType="panel"
              name="${./name}"
              jcr:title="${'./jcr:title'}"
              isTitleRichText="${./isTitleRichText}"
              hideTitle="${./hideTitle}"
              dataRef="${./bindRef}"
              visible="${./visible}"
              enabled="${./enabled}"
              readOnly="{Boolean}false"
              showApprovalOption="${./showApprovalOption}"
              showAsPopup="${./showAsPopup}"
              showLink="${./showLink}"
              wrapData="{Boolean}true"
              textIsRich="${./textIsRich}"
              tooltip="${./shortDescription}"
              tooltipVisible="${./shortVisible}"
              description="${./longDescription}"
              assistPriority="${./assistPriority}"
              custom="${./custom}">
        <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="appId/components/adaptiveForm/text"
                cq:rewriteFinal="{Boolean}true"
                fieldType="plain-text"
                name="text"
                jcr:title="Text"
                dataRef="${./bindRef}"
                value="${./tncTextContent}"
                textIsRich="true"/>
        <approvalcheckbox
                jcr:primaryType="nt:unstructured"
                sling:resourceType="appId/components/adaptiveForm/checkbox"
                cq:rewriteFinal="{Boolean}true"
                fieldType="checkbox"
                name="approvalcheckbox"
                jcr:title="${./tncCheckBoxContent}"
                checkedValue="true"
                enabled="true"
                required="${./required}"
                mandatoryMessage="${./mandatoryMessage}"
                validateExpMessage="${./validateExpMessage}"/>
        <link
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/fd/components/form/toggleablelink/v1/toggleablelink"
                cq:rewriteFinal="{Boolean}true"
                fieldType="checkbox-group"
                name="link"
                jcr:title="Link Title"
                title="${./tncCheckBoxContent}"
                enum="${./linkText}"
                enumNames="${./linkText}"
                orientation="vertical"
                readOnly="false">
          <fd:rules
                  jcr:primaryType="nt:unstructured"
                  validationStatus="valid"/>
          <fd:events
                  jcr:primaryType="nt:unstructured"
                  change="[if(length($field.$value) == length($field.$enum), dispatchEvent($parent.approvalcheckbox, 'custom:setProperty', {enabled : true()}), {})]"/>
        </link>
      </termsAndConditions>
    </replacement>
  </termsAndConditions>
